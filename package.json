{
  "name": "financiera-sifmex-monorepo",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "echo \"Ejecuta scripts propios en /server y /client\"",
    "db:generate": "pnpm exec prisma generate --schema=prisma/schema.prisma",
    "db:migrate":  "pnpm exec prisma migrate dev --schema=prisma/schema.prisma",
    "db:seed":     "pnpm exec node prisma/seedContacts.mjs"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "devDependencies": {
    "prisma": "^6.13.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@prisma/client": "6.13.0"
  }
}
