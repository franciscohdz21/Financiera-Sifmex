    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "seed:users": "node prisma/seedUsers.mjs",
    "seed:contacts": "node prisma/seedContacts.mjs",
    "clean": "rimraf node_modules"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.1.0",
    "@fastify/jwt": "^9.1.0",
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "prisma": "^5.19.1",
    "rimraf": "^6.0.1"
  }
}
