{
  "name": "financiera-sifmex-backend",
  "private": true,
  "version": "0.1.0",
  "description": "Backend Fastify + Prisma + PostgreSQL para Financiera-Sifmex",
  "author": "Francisco Hernandez",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "db:migrate": "prisma migrate dev",
